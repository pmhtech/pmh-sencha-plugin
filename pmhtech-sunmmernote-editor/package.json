{
    "name": "pmhtech-summernote-editor",
    "sencha": {
        "namespace": "PmhTech",
        "type": "theme",
        "extend": "theme-triton",
        "toolkit": "classic",
        "creator": "anonymous",
        "summary": "Short summary",
        "detailedDescription": "Long description of package",
        "version": "1.0.0",
        "compatVersion": "1.0.0",
        "format": "1",
        "slicer": {
            "js": [
                {
                    "path": "${package.dir}/sass/example/custom.js",
                    "isWidgetManifest": true
                }
            ]
        },
        "js" : [{
                "path": "${package.dir}/resources/jquery/jquery-3.5.1.js"
            },{
                "path": "${package.dir}/resources/summernote/summernote-lite.js"
            },{
            "path": "${package.dir}/resources/summernote/summernote-ko-KR.js"
        }
        ],
        "css" : [{
            "path": "${package.dir}/resources/summernote/summernote-lite.css"
        }],
        "resource": {
            "paths": ""
        },
        "resources": [
            {
                "path": "${package.dir}/resources",
                "output": "shared"
            }
        ],
        "output": "${package.dir}/build",
        "local": true,
        "sass" : {
            "namespace": "Ext",
            "etc": [
                "${package.dir}/sass/etc/all.scss"
             ],
            "var": [
                "${package.dir}/sass/var"
            ],
            "src": [
                "${package.dir}/sass/src"
            ]
        },
        "classpath": [
            "${package.dir}/src"
        ],
        "overrides": [
            "${package.dir}/overrides"
        ],
        "example": {
            "path": [
                "${package.dir}/examples"
            ]
        },
        "framework": "ext",
        "requires": ["pmhtech-classic"]
    }
}
